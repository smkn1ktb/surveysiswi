{
  "builds": [
    {
      "src": "**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "log.txt",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.php",
      "dest": "/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
